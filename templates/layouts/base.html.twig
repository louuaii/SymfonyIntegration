<!DOCTYPE html>
<html lang="en">

	<head>
		<title>
			{% block title %}E-Learning
			{% endblock %}
		</title>
		<meta charset="utf-8">
		<meta
		name="viewport" content="width=device-width, initial-scale=1">

		<!-- Favicon and Apple Icons -->
		<link rel="apple-touch-icon" href="{{ asset('assets/FO/img/apple-icon.png') }}">
		<link
		rel="shortcut icon" type="image/x-icon" href="{{ asset('assets/FO/img/favicon.ico') }}">

		<!-- Load Required Bootstrap CSS -->
		<link
		href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/css/bootstrap.min.css" rel="stylesheet">

		<!-- Font CSS -->
		<link
		href="https://fonts.googleapis.com/css2?family=Open+Sans:wght@300;400;600&display=swap" rel="stylesheet">

		<!-- Load Template CSS -->

		{% block stylesheets %}
			{{ encore_entry_link_tags('fo-styles') }}
		{% endblock %}
		<link rel="stylesheet" href="{{ asset('build/global.css') }}">
        <script src="{{ asset('build/app.js')}}"></script>
		{{ encore_entry_script_tags('fo-scripts') }}

	</head>

	<body>
		<!-- Header -->
		{{ include('partials/_nav.html.twig') }}
		<!-- Close Header -->
		<main role="main" class="container"> {% block body %}{% endblock %}
			</main>
			{{ include('partials/_footer.html.twig') }}
			{% include("partials/_alert.html.twig") %}
		</body>

	</html>
